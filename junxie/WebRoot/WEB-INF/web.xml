<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>junxie1</display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>TypeServlet</servlet-name>
    <servlet-class>com.my.junxie.servlet.TypeServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ArmServlet</servlet-name>
    <servlet-class>com.my.junxie.servlet.ArmServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>TypeServlet</servlet-name>
    <url-pattern>/type.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ArmServlet</servlet-name>
    <url-pattern>/arm.do</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>arm.do</welcome-file>
  </welcome-file-list>
</web-app>